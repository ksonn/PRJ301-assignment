<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AuthenticationController</servlet-name>
        <servlet-class>controller.AuthenticationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feature</servlet-name>
        <servlet-class>controller.Feature</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAppointment</servlet-name>
        <servlet-class>controller.Appointment.GetAppointment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAppointment</servlet-name>
        <servlet-class>controller.Appointment.ListAppointment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePatient</servlet-name>
        <servlet-class>controller.PatientController.DeletePatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPatient</servlet-name>
        <servlet-class>controller.PatientController.AddPatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchTreatment</servlet-name>
        <servlet-class>controller.Treatment.SearchTreatment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditTreatment</servlet-name>
        <servlet-class>controller.Treatment.EditTreatment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTreatment</servlet-name>
        <servlet-class>controller.Treatment.AddTreatment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchPatientById</servlet-name>
        <servlet-class>controller.PatientController.SearchPatientById</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetListEquipment</servlet-name>
        <servlet-class>controller.equipment.GetListEquipment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertEquipment</servlet-name>
        <servlet-class>controller.equipment.InsertEquipment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deteleEquipment</servlet-name>
        <servlet-class>controller.equipment.deteleEquipment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthenticationController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feature</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAppointment</servlet-name>
        <url-pattern>/getappointment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAppointment</servlet-name>
        <url-pattern>/listappointment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePatient</servlet-name>
        <url-pattern>/patient/detele</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPatient</servlet-name>
        <url-pattern>/patient/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchTreatment</servlet-name>
        <url-pattern>/treatment/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditTreatment</servlet-name>
        <url-pattern>/treatment/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTreatment</servlet-name>
        <url-pattern>/treatment/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchPatientById</servlet-name>
        <url-pattern>/patient/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetListEquipment</servlet-name>
        <url-pattern>/equipment/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEquipment</servlet-name>
        <url-pattern>/equipment/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deteleEquipment</servlet-name>
        <url-pattern>/deteleEquipment</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
